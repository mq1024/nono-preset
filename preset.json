{
  "useConfigFiles": true,
  "router": true,
  "vuex": false,
  "routerHistoryMode": false,
  "cssPreprocessor": "scss",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "recommended",
      "lintOn": ["save", "commit"]
